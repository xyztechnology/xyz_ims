@model Inventory.Models.TransactionAnalysisSetup

<div>
    <div class="modal-body">


        <span class="glyphicon glyphicon-sign"></span>
       
            @using (Ajax.BeginForm("CreateHead", "TransactionAnalysisSetup", null, new AjaxOptions { HttpMethod = "POST" }, new { id = "CreateHead" }))
    {

                @Html.HiddenFor(m => m.ParentId)
                <table>

                    <tr>
                        <td><label class="control-label col-md-2">Name</label></td>
                        <td>
                            @Html.EditorFor(model => model.Name)
                            @Html.ValidationMessageFor(model => model.Name)
                        </td>
                    </tr>
                    <tr></tr>

                </table>


        }


        </div>

    </div>

    <script type="text/javascript">
        $(function () {
            $('#approve-btn').click(function () {
                $('#modal-container').modal('hide');
            });
        });
    </script>

